{
  "name": "time-series-analysis-web-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "lite": "lite-server",
    "minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
    "copy:debug": "cpy debug.html ../ --cwd=src --rename=index.html",
    "copy:prod": "cpy production.html ../ --cwd=src --rename=index.html",
    "build:scss": "node-sass content/scss/_main.scss content/css/timeseriesanalysis.min.css --output-style compressed",
    "build:tsc:debug": "cpy tsconfig.base.json ./ --rename=tsconfig.json && tsc",
    "build:tsc:prod": "cpy tsconfig.prod.json ./ --rename=tsconfig.json && tsc",
    "build:debug": "npm run build:scss && npm run copy:debug && npm run build:tsc:debug",
    "build:prod": "npm run build:scss && npm run copy:prod && npm run build:tsc:prod && browserify -s main dist/Main.js > dist/bundle.js && npm run minify",
    "run:debug": "npm run build:debug && concurrently \"tsc -w\" \"npm run lite\" ",
    "run:prod": "npm run build:prod && npm run lite",
    "test:debug": "npm run build:tsc:debug && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test": "npm run build:tsc:debug && karma start karma.conf.js --single-run"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/router": "4.1.3",
    "@angular/upgrade": "4.1.3",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.4.0",
    "systemjs": "0.20.12",
    "zone.js": "0.8.11",
    "angular2-highcharts": "^0.5.5",
    "es6-shim": "^0.35.3",
    "jquery": "^3.2.1",
    "highcharts": "^5.0.11",
    "ngx-modal": "^0.0.29",
    "font-awesome": "^4.7.0",
    "lato-font": "^3.0.0",
    "es6-promise": "^4.1.0"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "browserify": "^14.3.0",
    "concurrently": "^3.4.0",
    "http-server": "^0.10.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.5.3",
    "typescript": "^2.3.2",
    "typings": "^2.1.1",
    "uglify-js": "^3.0.10",
    "cpy-cli": "^1.0.1",
    "jasmine-core": "^2.6.2",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-teamcity-reporter": "^1.0.0",
    "phantomjs-prebuilt": "^2.1.14",
    "karma-phantomjs-launcher": "^1.0.4",
    "watchify": "^3.9.0",
    "babel-polyfill": "^6.23.0"
  }
}